   nome: cadeia
   idade: inteiro
   altura: real

inicio
   escreva("Digite seu nome: ")
   leia(nome)

   escreva("Digite sua idade: ")
   leia(idade)

   escreva("Digite sua altura (ex: 1.75): ")
   leia(altura)

   escreval("Nome: ", nome)
   escreval("Idade: ", idade)
   escreval("Altura: ", altura)
fimalgoritmo
